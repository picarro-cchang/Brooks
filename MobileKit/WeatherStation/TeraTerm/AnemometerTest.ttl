inputbox 'Base serial port' 'COM Port Settings'
str2int base inputstr

gettime timestr "%Y%m%d-%H%M%S"

sprintf2 filename 'c:\temp\gps_%s.log' timestr
sprintf2 port '/C=%d' base
connect port
setbaud 19200
logopen filename 0 0 0 1 1
unlink

