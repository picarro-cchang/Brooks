#!/bin/bash

# Remove files and folders created on install
sudo rm -rf /usr/local/share/picarro >> /dev/null
sudo rm -rf /home/$SUDO_USER/.config/autostart/pigss-launcher.desktop >> /dev/null

# Remove pigss-launcher from favorite-apps list
dconf write /org/gnome/shell/favorite-apps "['']"

# Update icon cache
gtk-update-icon-cache
