 
{
    "extends": ["tslint:latest"],
    "rules": {
        "no-implicit-dependencies": [true, "dev"],
        "max-line-length": {
            "options": [120]
        },
        "new-parens": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "eofline": true
    }
  }
